{
  "name": "characters-api",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc -p ./tsconfig.json",
    "start:dev": "tsc-watch --noClear -p ./tsconfig.json --onSuccess \"node ./dist/app.js\"",
    "dynamodb:create": "npm run build && node ./dist/character/dynamodb.config.js create",
    "dynamodb:delete": "npm run build && node ./dist/character/dynamodb.config.js delete",
    "dynamodb:describe": "npm run build && node ./dist/character/dynamodb.config.js describe",
    "dynamodb:list": "npm run build && node ./dist/character/dynamodb.config.js list",
    "dynamodb:test": "npm run build && node ./dist/character/character.dynamodb.test.js test",
    "dynamodb:cleanup": "npm run build && node ./dist/character/character.dynamodb.test.js cleanup"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/express": "^5.0.2",
    "@types/node": "^22.15.21",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.830.0",
    "@aws-sdk/lib-dynamodb": "^3.830.0",
    "@types/pg": "^8.15.4",
    "express": "^5.1.0",
    "mongodb": "^6.17.0",
    "pg": "^8.16.0"
  }
}
